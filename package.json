{
  "name": "farm-manager",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:next\"",
    "dev:api": "nodemon --watch src server.js",
    "dev:next": "next dev --turbopack -p 3000",
    "build": "next build",
    "vercel-build": "npm run build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "test": "NODE_ENV=test DB_DIALECT=sqlite DB_STORAGE=\":memory:\" NODE_OPTIONS='--experimental-vm-modules --disable-warning=ExperimentalWarning' jest",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@vercel/analytics": "^1.5.0",
    "bcrypt": "^5.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^9.0.0",
    "lucide-react": "^0.542.0",
    "morgan": "^1.10.1",
    "next": "^15.5.9",
    "pdfkit": "^0.13.0",
    "pg": "^8.11.1",
    "react": "^19.0.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.0.0",
    "sequelize": "^6.32.1",
    "stream-buffers": "^3.0.2",
    "tailwind-merge": "^3.3.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {}
  }
}
