{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node@2.0.0"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "server.js": {
      "includeFiles": "src/**"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}